{
  "name": "kek-starter",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "yarn run up && yarn run build && node dist/app/server.js",
    "build": "babel app --out-dir dist/app --ignore \"**/*.test.js\" ",
    "start-dev": "nodemon --watch ./ --exec babel-node ./app/server.js --inspect=9222",
    "test": "exit 0",
    "up": "db-migrate up",
    "down-all": "db-migrate reset",
    "remigrate": "npm run down-all & npm run up",
    "down": "db-migrate down",
    "create-migration": "db-migrate create"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "db-migrate-pg": "^0.2.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "node-mailer": "^0.1.1",
    "pg-promise": "^7.2.1",
    "squel": "^5.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "db-migrate": "^0.10.0-beta.24",
    "nodemon": "^1.12.1"
  },
  "repository": "git@github.com:plnkvlchk/kek-starter.git",
  "author": "Poline Khadaryonok <p.khadaryonok@lovata.com>",
  "license": "MIT"
}
